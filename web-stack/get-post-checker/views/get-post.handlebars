<h1 class="bg-primary">{{this.typeName}} request received</h1>

<!-- Print table of key value pairs from query string if they exist -->
<div class="row">
    <div class="col-lg-12">
        {{#if queryKvPairs}}
            <table class="table table-striped table-responsive">
                <caption>Query String Parameters</caption>
                <thead>
                    <th>Key</th>        
                    <th>Value</th>
                </thead>
                {{#each queryKvPairs}}
                <tr>
                    <td>{{this.key}}</td>
                    <td>{{this.value}}</td>
                </tr>
                {{/each}}
            </table>
        {{else}}
            <p>No query string submitted</p>
        {{/if}}
    </div> <!-- End column -->
</div> <!-- End Query String row -->

<!-- Print POST body parameters if they exist -->
<div class="row">
    <div class="col-lg-12">
        {{#if postKvPairs}}
            <table class="table table-striped table-responsive">
                <caption>POST Body Parameters</caption>
                <thead>
                    <th>Key</th>        
                    <th>Value</th>
                </thead>
                {{#each postKvPairs}}
                <tr>
                    <td>{{this.key}}</td>
                    <td>{{this.value}}</td>
                </tr>
                {{/each}}
            </table>
        {{else}}
            <p>No req.body submitted</p>
        {{/if}}
    </div> <!-- End column -->
</div> <!-- End row -->